      </Fields>
      <Query>
        <DataSourceName>PurchaseItemDetail</DataSourceName>
        <CommandText>
-------------------
Select 
	T.TaskID,
	TaskName,
	DateDue,
	DateCompleted,
	Note,
	I.ItineraryName,
	U.UserName
From Task T
Left Join ItineraryTask IT on IT.TaskID = T.TaskID
Left Join Itinerary I on I.ItineraryID = IT.ItineraryID
Left Join [User] U on U.UserID = I.AssignedTo
Where DateCompleted is Null
	and DateDue &gt;= @StartDate
	and DateDue &lt; @EndDate
	AND (UserID IN @UserIDList) 
	and (I.ItineraryStatusID IN @ItineraryStatusIDList) 
Order By DateDue
-------------------		
		</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
