      <Query>
        <DataSourceName>PurchaseItemDetail</DataSourceName>
        <CommandText>
SELECT ItineraryID,isnull(DisplayName,ItineraryName) as ItineraryName,CustomCode,ItineraryLength,ArriveDate,ArriveCityName,DepartDate,DepartCityName,ItineraryGross,
ItineraryTotalPayments,ItineraryTotalOutstanding,PurchaseLineName,PurchaseItemName,PurchaseItemStartDate,CityName,NumberOfDays,Quantity,ServiceTypeName, 
isnull(ItineraryCurrencyFormat, 'c') as ItineraryCurrencyFormat
FROM dbo.PurchaseItemDetail p
WHERE ItineraryID = @ItineraryID AND (ServiceTypeID IN @ServiceTypeIDList) 
ORDER BY PurchaseItemStartDate, PurchaseItemStartTime
		</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
