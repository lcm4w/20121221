  <Code>Function WarnIfHasPriceOverride(total1 as Double, total2 as Double)	
	If (total1 &lt;&gt; total2) Then		
		MsgBox("This report will not show the adjusted final price of " + total1.ToString("C") + vbCrLf + vbCrLf + "Instead it shows all booking prices and total sum of " + total2.ToString("C"),0,"Override Warning")     
	End If
End Function
</Code>
